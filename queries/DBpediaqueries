# 1. Obtener vehículos seat de mas de 4 metros

SELECT ?vehicle WHERE {
  ?vehicle a dbo:Automobile .
  ?vehicle dbo:length ?length .
  ?vehicle dbo:manufacturer	dbr:SEAT
  FILTER(?length > 4)
}

# Wrong

SELECT ?vehicle WHERE {
  ?vehicle a dbo:Automobile .
  ?vehicle dbo:length ?length .
  ?vehicle dbo:manufacturer	dbr:SET
  FILTER(?length > 4)
}

#Expected http://dbpedia.org/resource/SEAT_1400

# 2. Obtener películas dirigidas por Christopher Nolan que duran más de 120 minutos
SELECT ?movie WHERE {
  ?movie a dbo:Film .
  ?movie dbo:runtime ?runtime .
  ?movie dbo:director dbr:Christopher_Nolan .
  FILTER (?runtime > 120) 
}

Wrong:
SELECT ?movie WHERE {
  ?movie a dbo:Movie .
  ?movie dbo:runtime ?runtime .
  ?movie dbo:director dbr:Christopher_Nolan .
  FILTER (?runtime > 120) 
}


# 3. Obtener libros escritos por J.K. Rowling de mas de 300 páginas
SELECT ?book WHERE {
  ?book a dbo:Book .
  ?book dbo:author dbr:J._K._Rowling .
  ?book dbo:numberOfPages ?pages .
  FILTER (?pages > 300)
}

# 4. Obtener ciudades con una población mayor a 1,000,000
SELECT ?city WHERE {
  ?city a dbo:City .
  ?city dbo:populationTotal ?population .
  FILTER(?population > 1000000)
}

# 5. Obtener países donde el español es un idioma oficial con mas de un millón de habitantes
SELECT ?country WHERE {
  ?country a dbo:Country .
  ?country dbo:officialLanguage dbr:Spanish_language .
  ?country dbo:population ?population .
  FILTER (?population > 1000000) 
}

# 6. Obtener empresas fundadas después del año 2000
SELECT ?company WHERE {
  ?company a dbo:Company .
  ?company dbo:foundingYear ?year .
  FILTER(?year > 2000)
}

# 7. Obtener ríos con una longitud mayor a 1000 km
SELECT ?river WHERE {
  ?river a dbo:River .
  ?river dbo:length ?length .
  FILTER(?length > 1000)
}

# 8. Obtener montañas con una altura mayor a 5000 metros
SELECT ?mountain WHERE {
  ?mountain a dbo:Mountain .
  ?mountain dbo:elevation ?elevation .
  FILTER(?elevation > 5000)
}

# 9. Obtener bandas de rock activas después del 2010
SELECT ?band WHERE {
  ?band a dbo:Band .
  ?band dbo:activeYearsEndDate ?endYear .
  FILTER(?endYear > 2010)
}

# 10. Obtener universidades fundadas antes del 1900
SELECT ?university WHERE {
  ?university a dbo:University .
  ?university dbo:foundingYear ?year .
  FILTER(?year < 1900)
}

# 11. Obtener museos en París
SELECT ?museum WHERE {
  ?museum a dbo:Museum .
  ?museum dbo:location dbr:Paris .
  ?museum dbo:visitorsPerYear ?visitors .  # 
  FILTER (?visitors > 100000)  
}

# 12. Obtener aeropuertos en Alemania
SELECT ?airport WHERE {
  ?airport a dbo:Airport .
  ?airport dbo:location dbr:Germany .
  ?airport dbo:elevation ?elevation .   
  FILTER (?elevation > 500)  
}

# 13. Obtener festivales de música celebrados en Reino Unido
SELECT ?festival WHERE {
  ?festival a dbo:MusicFestival .
  ?festival dbo:location dbr:United_Kingdom .
  ?festival dbo:attendance ?attendance .   
  FILTER (?attendance > 10000)  
}

# 14. Obtener actores nacidos en Londres
SELECT ?actor WHERE {
  ?actor a dbo:Actor .
  ?actor dbo:birthPlace dbr:London .
  ?actor dbo:height ?height .  
  FILTER (?height > 1.75) 
}

# 15. Obtener atletas olímpicos de Brasil
SELECT ?athlete WHERE {
  ?athlete a dbo:Athlete .
  ?athlete dbo:nationality dbr:Brazil .
  ?athlete dbo:height ?height .  
  FILTER (?height > 1.80) 
}

# 16. Obtener periódicos publicados en inglés
SELECT ?newspaper WHERE {
  ?newspaper a dbo:Newspaper .
  ?newspaper dbo:language dbr:English_language .
  ?newspaper dbo:circulation ?circulation .  
  FILTER (?circulation > 100000)  
}

# 17. Obtener islas con una superficie mayor a 1000 km²
SELECT ?island WHERE {
  ?island a dbo:Island .
  ?island dbo:areaTotal ?area .
  FILTER(?area > 1000)
}

# 18. Obtener compañías de tecnología fundadas en EE.UU.
SELECT ?company WHERE {
  ?company a dbo:Company .
  ?company dbo:industry dbr:Technology_industry .
  ?company dbo:locationCountry dbr:United_States .
  ?company dbo:numberOfEmployees ?employees .   
  ?company dbo:revenue ?revenue .   
  ?company dbo:foundingYear ?foundingYear .  
  FILTER (?employees > 10000 && ?revenue > 100000000 && ?foundingYear < 2000)  
}

# 19. Obtener escritores nacidos en Rusia
SELECT ?writer WHERE {
  ?writer a dbo:Writer .
  ?writer dbo:birthPlace dbr:Russia .
  ?writer dbo:deathPlace ?deathPlace .  
  ?writer dbo:nationality ?nationality .  
  ?writer dbo:numberOfBooks ?numberOfBooks .   
  FILTER (?numberOfBooks > 10)  
}

# 20. Obtener equipos de fútbol de Italia
SELECT ?team WHERE {
  ?team a dbo:SoccerClub .
  ?team dbo:league dbr:Serie_A .
  ?team dbo:ground ?ground .   
  ?team dbo:location  dbr:Rome .   
  ?team dbo:country dbr:Italy .  
  ?ground dbo:capacity ?capacity .  
  FILTER (?capacity > 50000)  
 
}

# 21. Obtener planetas del sistema solar
SELECT ?planet WHERE {
  ?planet a dbo:Planet .
  ?planet dbo:locatedInSystem dbr:Solar_System .
  ?planet dbo:mass ?mass .      
  ?planet dbo:meanRadius ?radius .   
  ?planet dbo:orbitalPeriod ?period .  
  ?planet dbo:surfaceGravity ?gravity . 
  FILTER (?mass > 1E24)          
  FILTER (?radius < 100000000)   
  FILTER (?period > 80)  
  FILTER (?gravity > 5)  
}

# 22. Obtener lenguajes de programación creados antes del 2000
SELECT ?language WHERE {
  ?language a dbo:ProgrammingLanguage .
  ?language dbo:year ?year .
  FILTER(?year < 2000)
}

# 23. Obtener especies de mamíferos
SELECT ?species WHERE {
  ?species a dbo:Mammal .
  ?species dbo:averageLifespan ?lifespan .  
  ?species dbo:maximumWeight ?weight .   
  ?species dbo:gestationPeriod ?gestation .  
  ?species dbo:numberOfOffspring ?offspring .  
  FILTER (?lifespan > 10)          
  FILTER (?weight < 2000)          
  FILTER (?gestation > 50)        
  FILTER (?offspring > 1) 
}

# 24. Obtener arquitectos nacidos en España
SELECT ?architect WHERE {
  ?architect a dbo:Architect .
  ?architect dbo:birthPlace dbr:Spain .
}

# 25. Obtener escritores españoles
SELECT ?architect WHERE {
  ?architect a dbo:Architect .
  ?architect dbo:birthPlace dbr:Spain .
  ?architect dbo:height ?height .        
  ?architect dbo:activePeriodStartYear ?startYear .  
  ?architect dbo:numberOfProjects ?projects .  
  
  FILTER (?height > 1.70)        
  FILTER (?startYear > 1950)       
  FILTER (?projects > 5)  
  FILTER (YEAR(?deathDate) < 2000)  
}

# 26. Películas con duración mayor a 120 minutos, director, país de producción y género
SELECT ?film ?duration ?director ?country ?genre WHERE {
  ?film a dbo:Film .
  ?film dbo:runtime ?duration .
  ?film dbo:director ?director .
  ?film dbo:country ?country .
  ?film dbo:genre ?genre .
  ?director foaf:name ?directorName .
  FILTER(?duration > 120)
}

# 27. Rascacielos con más de 300 metros de altura y ubicación
SELECT ?skyscraper ?height ?location WHERE {
  ?skyscraper a dbo:Skyscraper .
  ?skyscraper dbo:height ?height .
  ?skyscraper dbo:location ?location .
  ?location rdfs:label ?locationName .
  FILTER(?height > 300)
}

# 28. Películas con duración mayor a 120 minutos y género

SELECT ?film ?duration ?genre WHERE {
  ?film a dbo:Film .
  ?film dbo:runtime ?duration .
  ?film dbo:genre ?genre .
  ?genre rdfs:label ?genreName .
  FILTER(?duration > 120)
}

# 29. Empresas fundadas después del 2000 con sede y fundador

SELECT ?company ?founder ?headquarters WHERE {
  ?company a dbo:Company .
  ?company dbo:foundingYear ?year .
  ?company dbo:founder ?founder .
  ?company dbo:headquarter ?headquarters .
  FILTER(?year > 2000)
}

# 30. Países con área mayor a 1 millón de km² y continente
SELECT ?country ?area ?continent WHERE {
  ?country a dbo:Country .
  ?country dbo:areaTotal ?area .
  ?country dbo:continent ?continent .
  ?continent rdfs:label ?continentName .
  FILTER(?area > 1000000)
}

