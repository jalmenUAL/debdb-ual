}

# 22. Obtener lenguajes de programación creados antes del 2000
SELECT ?language WHERE {
  ?language a dbo:ProgrammingLanguage .
  ?language dbo:year ?year .
  FILTER(?year < 2000)
}

# 23. Obtener especies de mamíferos
SELECT ?species WHERE {
  ?species a dbo:Mammal .
}

# 24. Obtener arquitectos nacidos en España
SELECT ?architect WHERE {
  ?architect a dbo:Architect .
  ?architect dbo:birthPlace dbr:Spain .
}

# 25. Obtener escritores franceses
SELECT ?writer WHERE {
  ?writer a dbo:Writer .
  ?writer dbo:nationality dbr:France .
}

# 26. Películas con duración mayor a 120 minutos, director, país de producción y género
SELECT ?film ?duration ?director ?country ?genre WHERE {
  ?film a dbo:Film .
  ?film dbo:runtime ?duration .
  ?film dbo:director ?director .
  ?film dbo:country ?country .
  ?film dbo:genre ?genre .
  ?director foaf:name ?directorName .
  FILTER(?duration > 120)
}

# 27. Rascacielos con más de 300 metros de altura y ubicación
SELECT ?skyscraper ?height ?location WHERE {
  ?skyscraper a dbo:Skyscraper .
  ?skyscraper dbo:height ?height .
  ?skyscraper dbo:location ?location .
  ?location rdfs:label ?locationName .
  FILTER(?height > 300)
}

# 28. Películas con duración mayor a 120 minutos y género

SELECT ?film ?duration ?genre WHERE {
  ?film a dbo:Film .
  ?film dbo:runtime ?duration .
  ?film dbo:genre ?genre .
  ?genre rdfs:label ?genreName .
  FILTER(?duration > 120)
}

# 29. Empresas fundadas después del 2000 con sede y fundador

SELECT ?company ?founder ?headquarters WHERE {
  ?company a dbo:Company .
  ?company dbo:foundingYear ?year .
  ?company dbo:founder ?founder .
  ?company dbo:headquarter ?headquarters .
  FILTER(?year > 2000)
}

# 30. Países con área mayor a 1 millón de km² y continente
SELECT ?country ?area ?continent WHERE {
  ?country a dbo:Country .
  ?country dbo:areaTotal ?area .
  ?country dbo:continent ?continent .
  ?continent rdfs:label ?continentName .
  FILTER(?area > 1000000)
}

