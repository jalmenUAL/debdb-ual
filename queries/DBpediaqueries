# 1. SELECT ?vehicle WHERE {
  ?vehicle a dbo:Automobile .
  ?vehicle dbo:productionStartYear ?year .
  FILTER(?year > 2015)
}

# 2. Obtener películas dirigidas por Christopher Nolan
SELECT ?movie WHERE {
  ?movie a dbo:Film .
  ?movie dbo:director dbr:Christopher_Nolan .
}

# 3. Obtener libros escritos por J.K. Rowling
SELECT ?book WHERE {
  ?book a dbo:Book .
  ?book dbo:author dbr:J._K._Rowling .
}

# 4. Obtener ciudades con una población mayor a 1,000,000
SELECT ?city WHERE {
  ?city a dbo:City .
  ?city dbo:populationTotal ?population .
  FILTER(?population > 1000000)
}

# 5. Obtener países donde el español es un idioma oficial
SELECT ?country WHERE {
  ?country a dbo:Country .
  ?country dbo:officialLanguage dbr:Spanish_language .
}

# 6. Obtener empresas fundadas después del año 2000
SELECT ?company WHERE {
  ?company a dbo:Company .
  ?company dbo:foundingYear ?year .
  FILTER(?year > 2000)
}

# 7. Obtener ríos con una longitud mayor a 1000 km
SELECT ?river WHERE {
  ?river a dbo:River .
  ?river dbo:length ?length .
  FILTER(?length > 1000)
}

# 8. Obtener montañas con una altura mayor a 5000 metros
SELECT ?mountain WHERE {
  ?mountain a dbo:Mountain .
  ?mountain dbo:elevation ?elevation .
  FILTER(?elevation > 5000)
}

# 9. Obtener bandas de rock activas después del 2010
SELECT ?band WHERE {
  ?band a dbo:Band .
  ?band dbo:activeYearsEndDate ?endYear .
  FILTER(?endYear > 2010)
}

# 10. Obtener universidades fundadas antes del 1900
SELECT ?university WHERE {
  ?university a dbo:University .
  ?university dbo:foundingYear ?year .
  FILTER(?year < 1900)
}

# 11. Obtener museos en París
SELECT ?museum WHERE {
  ?museum a dbo:Museum .
  ?museum dbo:location dbr:Paris .
}

# 12. Obtener aeropuertos en Alemania
SELECT ?airport WHERE {
  ?airport a dbo:Airport .
  ?airport dbo:location dbr:Germany .
}

# 13. Obtener festivales de música celebrados en Reino Unido
