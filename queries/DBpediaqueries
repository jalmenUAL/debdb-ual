}

# 14. Obtener actores nacidos en Londres
SELECT ?actor WHERE {
  ?actor a dbo:Actor .
  ?actor dbo:birthPlace dbr:London .
}

# 15. Obtener atletas olímpicos de Brasil
SELECT ?athlete WHERE {
  ?athlete a dbo:Athlete .
  ?athlete dbo:nationality dbr:Brazil .
}

# 16. Obtener periódicos publicados en inglés
SELECT ?newspaper WHERE {
  ?newspaper a dbo:Newspaper .
  ?newspaper dbo:language dbr:English_language .
}

# 17. Obtener islas con una superficie mayor a 1000 km²
SELECT ?island WHERE {
  ?island a dbo:Island .
  ?island dbo:areaTotal ?area .
  FILTER(?area > 1000)
}

# 18. Obtener compañías de tecnología fundadas en EE.UU.
SELECT ?company WHERE {
  ?company a dbo:Company .
  ?company dbo:industry dbr:Technology_industry .
  ?company dbo:locationCountry dbr:United_States .
}

# 19. Obtener escritores nacidos en Rusia
SELECT ?writer WHERE {
  ?writer a dbo:Writer .
  ?writer dbo:birthPlace dbr:Russia .
}

# 20. Obtener equipos de fútbol de Italia
SELECT ?team WHERE {
  ?team a dbo:SoccerClub .
  ?team dbo:league dbr:Serie_A .
}

# 21. Obtener planetas del sistema solar
SELECT ?planet WHERE {
  ?planet a dbo:Planet .
